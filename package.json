{
  "name": "js-stack-from-scratch",
  "version": "2.4.5",
  "repository": "verekia/js-stack-boilerplate",
  "author": "Jonathan Verrecchia - @verekia",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": "8.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "cross-env NODE_ENV=production && rimraf lib dist && babel src -d lib --ignore .test.js && webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && jest --coverage",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build",
    "seed-database": "babel-node seed.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "dropzone": "^5.3.0",
    "express": "^4.15.0",
    "express-session": "^1.15.6",
    "immutable": "^4.0.0-rc.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "material-ui-search-bar": "^0.4.1",
    "mongoose": "^5.7.5",
    "multer": "^1.3.0",
    "nodemailer": "^4.4.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^4.2.7",
    "react-helmet": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-jss": "^5.3.0",
    "react-modal": "^3.1.11",
    "react-redux": "^5.0.3",
    "react-router": "4.0.0-beta.7",
    "react-router-dom": "4.0.0-beta.7",
    "react-tooltip": "^3.4.0",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.4.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "tether": "^1.4.0",
    "uuid": "^3.2.1",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.0.16",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.41.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
